{
    "name": "uepg/laravel-sybase",
    "type": "library",
    "description": "Sybase based Eloquent module extension for Laravel 5.x.",
    "authors": [
        {
            "name": "Gabriel Tadra Mainginski",
            "email": "13179626@uepg.br"
        },
        {
            "name": "Ademir Mazer Junior",
            "email": "mazer@uepg.br"
        }
    ],
    "keywords": [
        "sybase"
    ],
    "license": "GPL-2.0-only",
    "require": {
        "php": ">=5.6.4",
        "doctrine/dbal": "^2.9",
        "illuminate/database": "^5.4",
        "illuminate/support": "^5.4"
    },
    "require-dev": {},
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Uepg\\LaravelSybase\\SybaseServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "Uepg\\LaravelSybase\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
